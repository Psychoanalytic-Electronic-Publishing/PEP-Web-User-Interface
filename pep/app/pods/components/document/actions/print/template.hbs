<Button::WithTooltip
    @action={{this.print}}
    @type="link"
    @icon="print"
    @iconSize="lg"
    @iconPrefix="{{if @enabled 'fas' 'fal'}}"
    @tooltip={{unless @enabled (html-safe this.configuration.content.global.access.print.help)}}
    title={{if @enabled (t "common.print")}}
    class="p-1"
/>